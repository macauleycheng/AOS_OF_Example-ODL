<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<flow xmlns="urn:opendaylight:flow:inventory">
	<priority>300</priority>
	<flow-name>acl-action</flow-name>
	<table_id>60</table_id>
	<id>3</id>
	<match>
		<in-port>30</in-port>
		<vlan-match>
			<vlan-id>
				<vlan-id>60</vlan-id>
				<vlan-id-present>true</vlan-id-present>
			</vlan-id>
		</vlan-match>
		<ethernet-match>
			<ethernet-type>
				<type>2048</type>
			</ethernet-type>
			<ethernet-source>
				<address>00:00:00:00:23:ae</address>
			</ethernet-source>
			<ethernet-destination>
				<address>ff:ff:ff:ff:ff:ff</address>
			</ethernet-destination>
		</ethernet-match>
		<ipv4-source>192.168.1.13/24</ipv4-source>
		<ipv4-destination>192.168.0.6/24</ipv4-destination>	
		<ip-match>
			<ip-protocol>6</ip-protocol>
			<ip-dscp>2</ip-dscp>
			<ip-ecn>2</ip-ecn>
		</ip-match>
		<tcp-source-port>25364</tcp-source-port>
		<tcp-destination-port>8080</tcp-destination-port>
	</match>
	<instructions>
		<instruction>
			<order>0</order>
			<apply-actions>
				<action>
					<output-action>
						<output-node-connector>CONTROLLER</output-node-connector>
					</output-action>
					</action>
			</apply-actions>
		</instruction>
	</instructions>
</flow>